{
  "name": "vivacedemo",
  "version": "1.1.0",
  "description": "npm包规范示例",
  "main": "dist/vivacedemo.umd.min.js",
  "module": "dist/vivacedemo.esm.js",
  "unpkg": "dist/vivacedemo.js",
  "scripts": {
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "dev": "rollup -w --environment NODE_ENV:development -c scripts/rollup.config.dev.js",
    "build": "rollup --environment NODE_ENV:development -c scripts/rollup.config.prod.js",
    "prepare": "husky install",
    "commitlint": "commitlint --config commitlint.config.js -e -V",
    "commit": "git-cz",
    "lint": "eslint src",
    "fix": "eslint src --fix",
    "lint-staged": "lint-staged",
    "test": "jest",
    "test:c": "jest --coverage",
    "major": "npm version major -m 'build: update major'",
    "minor": "npm version minor -m 'build: update minor'",
    "patch": "npm version patch -m 'build: update patch'",
    "pub": "npm run build && npm publish --access=public",
    "pub:major": "npm run major && npm publish --access=public",
    "pub:minor": "npm run minor && npm publish --access=public",
    "pub:patch": "npm run patch && npm publish --access=public",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "createlog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
  },
  "jest": {
    "preset": "rollup-jest"
  },
  "lint-staged": {
    "src/*.js": [
      "prettier --write",
      "eslint --cache --fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/fe-test1/git-demo.git"
  },
  "author": "xiumubai",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/fe-test1/git-demo/issues"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  },
  "files": [
    "dist",
    "examples",
    "src"
  ],
  "homepage": "https://gitlab.com/fe-test1/git-demo#readme",
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/preset-env": "^7.19.3",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-eslint": "^8.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^14.1.0",
    "@rollup/plugin-replace": "^4.0.0",
    "commitizen": "^4.2.5",
    "conventional-changelog-cli": "^2.2.2",
    "cz-customizable": "^7.0.0",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.5.1",
    "husky": "^8.0.0",
    "jest": "^29.1.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "rollup-jest": "^3.0.0",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-filesize": "^9.1.2",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-serve": "^2.0.1",
    "rollup-plugin-terser": "^7.0.2"
  },
  "dependencies": {
    "rollup": "^2.79.1",
    "vitepress": "^1.0.0-alpha.17"
  }
}